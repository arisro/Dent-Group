<div class="sidebar sidebar-left sidebar-chat" id="chatContainer" ng-controller="ChatController">
  <div class="search-box">
    <div class="chat-toggle">
        <a id="chat-toggle-link" href="" title="Toggle chat"><i class="glyphicon glyphicon-circle-arrow-left"></i></a>
    </div>
    <input class='form-control' type="text" placeholder="Search" />
  </div>
  <hr />
  <div class="chat-box">
    <div id="chatbox" data-uid="<%= current_user.id %>" data-key="<%= Digest::MD5.hexdigest("#{current_user.id}saltlol") %>">

    </div>
    {{zzz}}
    <ul class="chat-list">
      <li class="chat-user-item" ng-repeat="user in users">
        <a class="chat-link" href="#{{user.user_id}}" title="Click to chat">
          <img src="" class="chat-item-image" />
          <span class="chat-user-name">{{user.user_name}}</span>
          <br/>
          <span class="chat-user-specialization">{{user.specialization}}</span>
        </a>
      </li>
    </ul>
  </div>
</div>